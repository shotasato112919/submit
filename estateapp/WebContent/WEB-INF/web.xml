<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchController</servlet-name>
  	<display-name>SearchController</display-name>
  	<description></description>
  	<servlet-class>estateapp.controller.SearchController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TopController</servlet-name>
  	<display-name>TopController</display-name>
  	<description></description>
  	<servlet-class>estateapp.controller.TopController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>SearchController</servlet-name>
  	<url-pattern>/SearchController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TopController</servlet-name>
  	<url-pattern>/TopController</url-pattern>
  </servlet-mapping>
</web-app>
